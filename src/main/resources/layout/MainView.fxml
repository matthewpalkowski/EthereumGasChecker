<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane
        id="MainViewPane"
        fx:id="MainViewPane"
        maxHeight="-Infinity"
        maxWidth="-Infinity"
        minHeight="-Infinity"
        minWidth="-Infinity"
        prefHeight="230.0"
        prefWidth="300.0"
        xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">

   <children>
      <Label id="lblTitle" fx:id="lblTitle" layoutX="24.5" layoutY="10.0" text="Ethereum Gas Checker">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label id="lblCredit" fx:id="lblCredit" layoutX="85.0" layoutY="42.5" text="Powered by Etherscan.io">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <Button id="btnGasScanning" fx:id="btnGasScanning" layoutX="100.0" layoutY="80.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="35.0" prefWidth="100.0" text="Scan gas prices" />
      <Button id="btnSetPreferences" fx:id="btnSetPreferences" layoutX="100.0" layoutY="140.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="35.0" prefWidth="100.0" text="Set Preferences" />
      <Label id="lblCurrentPricePrefix" fx:id="lblCurrentPricePrefix" layoutX="55.0" layoutY="195.0" text="Current Ethereum Price:">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Label id="lblCurrentPrice" fx:id="lblCurrentPrice" layoutX="205.0" layoutY="195.0" text="UNK">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
